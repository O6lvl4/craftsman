{
  "name": "craftsman",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "cli": "node bin/craftsman.js",
    "install:global": "npm i -g .",
    "link:global": "npm link",
    "unlink:global": "npm unlink -g craftsman || true && npm unlink || true",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:e2e": "NODE_OPTIONS='--experimental-vm-modules' jest --testMatch='**/test/e2e/**/*.test.js'"
  },
  "bin": {
    "craftsman": "bin/craftsman.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  }
}
